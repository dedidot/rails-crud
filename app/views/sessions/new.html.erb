<%= form_tag :sessions, action: :create do %>
    <div>
    <%= label_tag :email %>
    </div>

    <div>
    <%= email_field_tag :email %>
    </div>
    
    <div>
    <%= label_tag :password %>
    </div>

    <div>
    <%= password_field_tag :password %>
    </div>

    <div>
    <%= submit_tag "Login" %>
    </div>
    
    
<% end %>