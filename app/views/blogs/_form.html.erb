<%= form_for(@blog) do |f|  %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    </br>
    <%= f.label :user_id%>
    <%= f.select :user_id, options_for_select(@user), prompt: true, required: true %>
    </br>
    <%= f.label :description %>
    <%= f.text_area :description %>
    </br>
    <%= f.submit "submit" %>
<% end %>